<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="403.0" prefWidth="568.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.table.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="391.0" prefWidth="512.0">
         <children>
            <Label layoutX="40.0" layoutY="59.0" text="Nombre" />
            <Label layoutX="42.0" layoutY="107.0" text="Apellido" />
            <Label layoutX="51.0" layoutY="156.0" text="Edad" />
            <TextField fx:id="txtNombre" layoutX="106.0" layoutY="55.0" />
            <TextField fx:id="txtApellido" layoutX="108.0" layoutY="103.0" />
            <TextField fx:id="txtEdad" layoutX="108.0" layoutY="152.0" />
            <TableView fx:id="tb1Personas" layoutX="293.0" layoutY="56.0" prefHeight="209.0" prefWidth="225.0">
              <columns>
                <TableColumn fx:id="colNombre" prefWidth="75.0" text="Nombre" />
                <TableColumn fx:id="colApellidos" prefWidth="75.0" text="Apellido" />
                  <TableColumn fx:id="colEdad" prefWidth="75.0" text="Edad" />
              </columns>
            </TableView>
            <Button fx:id="btAgregar" layoutX="90.0" layoutY="253.0" mnemonicParsing="false" onAction="#agregarPersona" text="Agregar Persona" />
         </children></AnchorPane>
   </children>
</VBox>
